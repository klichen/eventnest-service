{
  "name": "server",
  "version": "1.0.0",
  "description": "eventnest_service",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch ./src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node22 --outfile=dist/index.cjs --external:swagger-ui-express",
    "typecheck": "tsc --noEmit",
    "start": "node ./dist/index.cjs",
    "db:migrate": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "populateBase": "tsx ./src/services/sop/populateAreasOfInterestTable.ts && tsx ./src/services/sop/populateCampusesTable.ts && tsx ./src/services/sop/populateClubs.ts",
    "populateCampuses": "tsx ./src/services/sop/populateCampusesTable.ts",
    "testFunc": "tsx ./src/utils/testFunctions.ts",
    "testAI": "tsx ./src/services/posts-processing/openaiTest.ts",
    "generateNewApiKey": "tsx ./src/utils/generateApiKey.ts --client madlab --env dev"
  },
  "author": "kebin",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@types/cors": "^2.8.17",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "luxon": "^3.6.1",
    "openai": "^5.2.0",
    "pg": "^8.14.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.13.11",
    "@types/pg": "^8.11.12",
    "@types/swagger-ui-express": "^4.1.8",
    "drizzle-kit": "^0.30.5",
    "esbuild": "^0.25.9",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}